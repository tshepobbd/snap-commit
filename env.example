# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=500

# Commit Message Configuration
COMMIT_MAX_LENGTH=72
COMMIT_DEFAULT_TYPE=chore
ENABLE_CONVENTIONAL_COMMITS=true

# CLI Configuration
DEFAULT_MESSAGE_COUNT=3
TEMP_FILE_PREFIX=commit-msg

# Git Configuration (optional, uses defaults if not set)
GIT_DIFF_COMMAND=git diff --cached
GIT_COMMIT_COMMAND=git commit

# Development
NODE_ENV=development
